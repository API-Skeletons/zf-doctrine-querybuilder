{
    "name": "zfcampus/zf-doctrine-querybuilder",
    "type": "library",
    "description": "QueryBuilder filters for Doctrine",
    "keywords": [
        "api",
        "apigility",
        "doctrine",
        "filter",
        "module",
        "zend",
        "zf2"
    ],
    "homepage": "https://github.com/zfcampus/zf-doctrine-querybuilder-filter",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Tom Anderson",
            "email": "tom.h.anderson@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "doctrine/doctrine-module": "^0.8 || ^1.0",
        "zendframework/zend-modulemanager": "^2.4",
        "zendframework/zend-mvc": "^2.4 || ^3.0",
        "zendframework/zend-servicemanager": "^2.7 || ^3.1",
        "zendframework/zend-hydrator":       "^1.1 || ^2.2.1",
        "zfcampus/zf-api-problem": "^1.1",
        "zfcampus/zf-hal": "^1.2"
    },
    "require-dev": {
        "doctrine/doctrine-mongo-odm-module": "0.*@dev",
        "doctrine/doctrine-orm-module": "^1.0",
        "zendframework/zend-test": "^2.5 || ^3.0",
        "phpunit/phpunit": "^4.1 || ^5.0",
        "zfcampus/zf-apigility-provider": "^1.1",
        "squizlabs/php_codesniffer": "^2.0"
    },
    "suggest": {
        "ext/mongo": "Mongo extension, if using ODM",
        "doctrine/doctrine-mongo-odm-module": "v0.*, if you wish to use the Doctrine ODM",
        "doctrine/doctrine-orm-module": "v1.*, if you wish to use the Doctrine ORM"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ZF\\Doctrine\\QueryBuilder\\": "src/"
        },
        "classmap": [
          "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ZFTest\\Doctrine\\QueryBuilder\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        },
        "zf": {
            "module": [
                "ZF\\Doctrine\\QueryBuilder"
            ]
        }
    }
}
